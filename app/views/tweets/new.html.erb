<div class="devise">
    <%= form_with model: Tweet, local: true do |f|%>
      <h3>
        投稿する
      </h3>
  <%= f.label :image %><br>
  <%= f.text_field :image ,placeholder: "URLを入力" %><br>
  <%= f.label :grade %><br>
  <%= f.text_field :grade ,placeholder: "GRADEを入力" %><br>
  <%= f.label :text %><br>
  <%= f.text_area :text ,placeholder: "本文を入力" %>
  <br>
  <%= f.submit :登録 %>
  <% end %>
</div>