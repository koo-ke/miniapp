      <div class="content_post"
      style=
      "background-size:cover;
       background-image:url(<%= tweet.image %>);">
       <span class="name"><%= tweet.user.nickname %></span>
       <% if user_signed_in? && current_user.id == tweet.user_id %>
       <div class="edit">
       <%= link_to '削除', "/tweets/#{tweet.id}", method: :delete %>
       <%= link_to '編集', "/tweets/#{tweet.id}/edit", method: :get %></div>
       <% end %>
        <span class="grade"><%= tweet.grade %></span>
        <span class="text"><%= simple_format(tweet.text) %></span>
      </div>